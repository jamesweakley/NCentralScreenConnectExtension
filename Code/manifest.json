{
    "name": "N-Central ScreenConnect extension",
    "version": "1.0.3",
    "manifest_version": 2,
    "description" : "Inserts ScreenConnect remote control icons into N-Central pages and provides a popup ScreenConnect iframe.",
    "icons": { "16": "icon_16.png", "48": "icon_48.png", "128": "icon_128.png" },
	"permissions": [ "storage", "webRequest",  "http://*/*IndexAction.action","https://*/*IndexAction.action"],
	"options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
	"web_accessible_resources": ["injected.js","injector.js","options.js"]
}